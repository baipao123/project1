.page.isPrompt{
    background-color: #fff;
}

image.icon{
    height:80rpx;
    width:80rpx;
    border-radius:60rpx;
    margin-top:12rpx;
}
.text-line.r{
    max-width: 450rpx;
}


.main {
    position: fixed;
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, .3);
}

.contain {
    position: absolute;
    display: block;
    background-color: #fff;
    top: 50%;
    left: 50%;
    width: 600rpx;
    -webkit-transform: translate(-50%, -50%);
    -moz-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
    padding: 0 30rpx;
    border-radius: 30rpx;
}
.contain .title{
    border-bottom: 1rpx solid #eee;
    padding: 15rpx 0;
}
.contain .body{
    padding:20rpx 0;
    border-bottom: 1rpx solid #eee;
}
.contain .footer{
    padding:15rpx 0;
}
.contain .footer .cancel{
    border-right: 1rpx solid #eee;
}
.contain .footer button{
    background-color: #fff;
    margin:0 !important;
    padding:0 !important;
    border:none;
    box-sizing: content-box !important;
    line-height: inherit !important;
    font-size: inherit !important;
}
.contain .footer button:after{
    border:none;
}