.header{
    position: absolute;
    bottom:500rpx;
    left:0;
    width:100%;
    height:60rpx;
    display:-webkit-box;
    display:-webkit-flex;
    display:flex;
    text-align: center;
}
.cancel,.submit{
    -webkit-box-flex:1;
    -webkit-flex:1;
    flex:1;
    color:#00aa00;
}
.title{
    -webkit-box-flex:2;
    -webkit-flex:2;
    flex:2;
}
.select{
    position: absolute;
    bottom:0;
    left:0;
    height:500rpx;
    width:100%;
    background-color: #ddd;
    text-align: center;
}