.prompt-page {
    position: fixed;
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, .3);
    left:0;
    top:0;
    z-index: 9999;
}
.prompt-contain {
    position: absolute;
    display: block;
    background-color: #fff;
    top: 50%;
    left: 50%;
    width: 600rpx;
    -webkit-transform: translate(-50%, -50%);
    -moz-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
    padding: 0 30rpx;
    border-radius: 30rpx;
}
.prompt-contain .title{
    border-bottom: 1rpx solid #eee;
    padding: 15rpx 0;
}
.prompt-contain .body{
    padding:20rpx 0;
    border-bottom: 1rpx solid #eee;
}
.prompt-contain .footer{
    padding:15rpx 0;
}
.prompt-contain .footer .cancel{
    border-right: 1rpx solid #eee;
}
.prompt-contain .footer button{
    background-color: #fff;
    margin:0 !important;
    padding:0 !important;
    border:none;
    box-sizing: content-box !important;
    line-height: inherit !important;
    font-size: inherit !important;
}
.prompt-contain .footer button:after{
    border:none;
}